{
  "name": "ADA",
  "description": "AAA bb",
  "version": "1.0",
  "browser_action": {
    "default_popup": "hello.html"
  },
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "file:///*/*"
  ], 
  "content_scripts": [
    {
      "matches": [
        "file:///C:/Users/*"
      ],
      "js": [ "contextscript.js" ],
      "run_at": "document_end"
    }
  ]
}
